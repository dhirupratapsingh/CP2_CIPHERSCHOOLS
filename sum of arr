#include<bits/stdc++.h>
using namespace std;
int main()
{
	int n,m;
	cin>>n>>m;
	int a[n],b[m];
	int num1=0,num2=0;
	for(int i=0;i<n;i++)
	{
	cin>>a[i];	
	num1+=a[i];
	num1=num1*10;
	}
	num1=num1/10;
		for(int i=0;i<m;i++)
	{
	cin>>b[i];
		num2+=b[i];
	num2=num2*10;	
	}
	num2=num2/10;	
cout<<num1+num2<<endl;
}
